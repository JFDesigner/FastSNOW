{
  "name": "FastSNOW",
  "description": "Takes the values from after the hash value in a url and inserts them into the corresponding form IDs.",
  "version": "1.1",
  "permissions": [
    "activeTab"
  ],
  "background": {
  "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "Parse the Hash values",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "manifest_version": 2,

  "content_scripts": [
    {
      "matches": ["https://bournemouth.service-now.com/com.glideapp.servicecatalog_cat_item_view.do?v=1&sysparm_id=3f1dd0320a0a0b99000a53f7604a2ef9*"],
      "js": ["parse.js"]
    }
  ]
}
